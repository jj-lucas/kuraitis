              <small>&copy; {{ 'now' | date: "%Y" }}, {{ langify_shop_name }}. {{ powered_by_link }} {% comment %}ly_i18n_replace_for_[ 'layout.footer.footer_line' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'layout.footer.footer_line' | t }}{% endcapture %}{% include 'ly-static-string' with '3334912' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} <a href="mailto:{{ shop.email }}">{{ shop.email }}</a></small>
            </div>
            <div class="grid__item medium-up--one-half medium-up--text-right">
              {% unless shop.enabled_payment_types == empty %}
                {% assign payment_icons_available = 'amazon_payments,american_express,bitcoin,cirrus,dankort,diners_club,discover,dogecoin,dwolla,forbrugsforeningen,google,interac,jcb,laser,litecoin,maestro,master,paypal,solo,stripe,switch,visa' | split: ',' %}
                <ul class="list--inline payment-icons">
                  {% for type in shop.enabled_payment_types %}
                    {% if payment_icons_available contains type %}
                      <li>
                        {% assign icon_name = type | prepend: 'icon-' %}
                        {% include icon_name %}
                        <span class="icon__fallback-text">{{type | replace: '_',' '}}</span>
                      </li>
                    {% endif %}
                  {% endfor %}
                </ul>
              {% endunless %}
            </div>
          </div>
        </div>
      </div>
    </footer>
  </div>

  {% comment %}ly_asset_replace_for_[ 'theme.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'theme.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}

  {% if template contains 'product' or template contains 'cart' %}
    {{ 'option_selection.js' | shopify_asset_url | script_tag }}
    <script>
    theme.moneyFormat = {{ shop.money_format | json }};
    </script>
  {% endif %}

  {% if template contains 'product' %}
    <script>
      // Pre-loading product images, to avoid a lag when a thumbnail is clicked, or
      // when a variant is selected that has a variant image.
      Shopify.Image.preload({{ product.images | json }}, 'large');

      // Set a global variable for theme.js to use on product single init
      theme.productSingleObject = {{ product | json }};
      {% if settings.product_stock_enable %}
        {% for variant in product.variants %}
          theme.productSingleObject.variants[{{ forloop.index0}}].incoming = {{ variant.incoming | default: false }};
          theme.productSingleObject.variants[{{ forloop.index0}}].next_incoming_date = {{ variant.next_incoming_date | date: format: 'month_day_year' | json }};
        {% endfor %}
      {% endif %}
    </script>
  {% endif %}

  <script id="QuantityTemplate" type="text/template">
    {% raw %}
      <div class="js-qty">
        <input type="text" value="{{ value }}" id="{{ id }}" name="{{ name }}" pattern="[0-9]*" data-line="{{ line }}" class="js-qty__input" aria-live="polite">
        <button type="button" class="js-qty__adjust js-qty__adjust--minus" aria-label="{% endraw %}{% comment %}ly_i18n_replace_for_[ 'cart.label.decrease_quantity' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.label.decrease_quantity' | t }}{% endcapture %}{% include 'ly-static-string' with '3334913' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}{% raw %}">
          {% endraw %}{% include 'icon-minus' %}{% raw %}
          <span class="icon__fallback-text"></span>
        </button>
        <button type="button" class="js-qty__adjust js-qty__adjust--plus" aria-label="{% endraw %}{% comment %}ly_i18n_replace_for_[ 'cart.label.increase_quantity' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'cart.label.increase_quantity' | t }}{% endcapture %}{% include 'ly-static-string' with '3334914' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}{% raw %}">
          {% endraw %}{% include 'icon-plus' %}{% raw %}
          <span class="icon__fallback-text">+</span>
        </button>
      </div>
    {% endraw %}
  </script>

  {% if template contains 'product' %}
    <div id="ProductModal" class="modal">
      <div class="modal__inner">
        <img src="" id="ProductZoomImg" class="product-modal__image">
      </div>
      <button type="button" class="modal__close js-modal-close text-link">
        {% include 'icon-close' %}
        <span class="icon__fallback-text">{% comment %}ly_i18n_replace_for_[ 'general.accessibility.close_modal' | t | json ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.accessibility.close_modal' | t | json }}{% endcapture %}{% include 'ly-static-string' with '3334915' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | json }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
      </button>
    </div>
  {% endif %}
{% comment %}ly_static_code_begin{% endcomment %}{% include 'ly-languages' %}{% comment %}ly_static_code_end{% endcomment %}{% include 'geolizr-currency-converter' %}

<script>
  GeolizrAPI.addEventListener('geolizr.geoData', function(geoData) {
    var countryToLanguages = {
      "DK": "ly24939"
    }
    
    var onChangeLanguage = function() {
      Geolizr.cookie('manually_selected_language', true);
    }
    $('#ly-languages-switcher').change(onChangeLanguage);
    $('.ly-languages-switcher-link').click(onChangeLanguage);
    var languageId = countryToLanguages[geoData.country.code];
    if(!Geolizr.cookie('manually_selected_language') && languageId && languageId != "{{ language }}") {
      langify.helper.saveLanguage(languageId);
    }
  });
</script>

</body>
</html>
