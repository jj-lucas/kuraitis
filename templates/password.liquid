{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}{% layout none %}
<!doctype html>
<!--[if IE 9]> <html class="ie9 supports-no-js" lang="{{ shop.locale }}"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <html class="supports-no-js" lang="{{ shop.locale }}"> <!--<![endif]-->
<head>
    <meta charset="UTF-8">
    <title>{{ langify_shop_name }}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="{{ settings.color_primary }}">

    {% if settings.favicon_enable %}
      <link rel="shortcut icon" href="{% comment %}ly_asset_replace_for_[ 'favicon.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'favicon.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" type="image/png" />
    {% endif %}

    <link rel="canonical" href="{{ canonical_url }}">

    {% include 'social-meta-tags' %}

    {% comment %}ly_asset_replace_for_[ 'theme.scss.css' | asset_url | stylesheet_tag ]_begin{% endcomment %}{% assign ly_asset = 'theme.scss.css' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | stylesheet_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
    {% include 'google-fonts' %}

    {{ content_for_header }}

    {% comment %}ly_asset_replace_for_[ 'modernizr.min.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'modernizr.min.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
    {{ '//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js' | script_tag }}

  </head>

  <body class="template-password">

    <a href="#LoginModal" class="password-login-link js-modal-open-login-modal">
      {% include 'icon-lock' %}
      {% comment %}ly_i18n_replace_for_[ 'general.password_page.password_link' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.password_link' | t }}{% endcapture %}{% include 'ly-static-string' with '3335136' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
    </a>

    <div class="password-page page-width text-center" role="main">
      <div class="password-page__inner">
        <div class="password-page__content">
          <div class="password-logo">
            {% if settings.logo_use_image %}
              <img class="password-logo__image" src="{% comment %}ly_asset_replace_for_[ 'logo.png' | asset_url ]_begin{% endcomment %}{% assign ly_asset = 'logo.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" alt="{{ langify_shop_name }}" itemprop="logo">
            {% else %}
              <h1 class="h2">
                <span>{{ langify_shop_name }}</span>
              </h1>
            {% endif %}
          </div>

          <h2 class="password-page__title h1">{% comment %}ly_i18n_replace_for_[ 'general.password_page.opening_soon' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.opening_soon' | t }}{% endcapture %}{% include 'ly-static-string' with '3335137' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h2>

          {% comment %}
            Password message which you can edit on /admin/online_store/preferences
          {% endcomment %}
          {% unless shop.password_message == blank %}
            <p class="password-page__message">
              {{ shop.password_message }}
            </p>
          {% endunless %}

          {% comment %}
            Signup form
          {% endcomment %}
          <div class="password-signup-form">
            {% form 'customer' %}
              {{ form.errors | default_errors }}
              {% if form.posted_successfully? %}
                <ul class="form-success">
                  <li>{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_success' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_success' | t }}{% endcapture %}{% include 'ly-static-string' with '3335138' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</li>
                </ul>
              {% else %}
                <p class="h6">{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_heading' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_heading' | t }}{% endcapture %}{% include 'ly-static-string' with '3335139' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
                <input type="hidden" name="contact[tags]" value="prospect, password page">
                <label for="email" class="label--hidden">{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_email_label' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_email_label' | t }}{% endcapture %}{% include 'ly-static-string' with '3335140' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
                <div class="input-group">
                  <input type="email" name="contact[email]" id="Email" class="input-group__field" placeholder="{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_email_placeholder' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_email_placeholder' | t }}{% endcapture %}{% include 'ly-static-string' with '3335141' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
                  <span class="input-group__btn">
                    <button type="submit" name="commit" class="btn btn--narrow">
                      {% include 'icon-arrow-right' %}
                      <span class="icon__fallback-text">{% comment %}ly_i18n_replace_for_[ 'general.password_page.signup_form_submit' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.signup_form_submit' | t }}{% endcapture %}{% include 'ly-static-string' with '3335142' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
                    </button>
                  </span>
                </div>
              {% endif %}
            {% endform %}
          </div>

          {% comment %}
            Share homepage URL
            Shared text is from the store meta description
          {% endcomment %}
          {% if settings.share_facebook or settings.share_twitter or settings.share_google %}
            <div class="password-social-sharing">
              <p class="h6">{% comment %}ly_i18n_replace_for_[ 'general.password_page.spread_the_word' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.spread_the_word' | t }}{% endcapture %}{% include 'ly-static-string' with '3335143' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
              {% include 'social-sharing', center_icons: true %}
            </div>
          {% endif %}

          <footer class="password-footer" role="contentinfo">
            {% capture shopify %}
              <a href="//www.shopify.com" target="_blank" title="<ly-as-3335135>Create your own online store with Shopify</ly-as-3335135>">
                {% include 'icon-shopify-logo' %}
                <span class="visually-hidden"><ly-as-3335134>Shopify</ly-as-3335134></span>
              </a>
            {% endcapture %}
            {% comment %}ly_i18n_replace_for_[ 'general.password_page.powered_by_shopify_html' | t: shopify: shopify ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.powered_by_shopify_html' | t: shopify: shopify }}{% endcapture %}{% include 'ly-static-string' with '3335145' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% assign replacement = shopify  %}{% assign ly_template = ly_template | replace: '[[shopify]]', replacement %}{% if escape_content %}{{ ly_template}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
          </footer>

        </div>
      </div>
    </div>

    <div id="LoginModal" class="modal">
      <div class="modal__inner">
        <div class="modal__centered password-modal__content page-width text-center">
          <p class="h4">{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_heading' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_heading' | t }}{% endcapture %}{% include 'ly-static-string' with '3335146' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
          <div class="password-login-form">
            {% form 'storefront_password' %}
              {{ form.errors | default_errors }}
              <label for="password" class="label--hidden">{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_password_label' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_password_label' | t }}{% endcapture %}{% include 'ly-static-string' with '3335147' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
              <div class="input-group">
                <input type="password" name="password" id="Password" class="input-group__field input--content-color" placeholder="{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_password_placeholder' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_password_placeholder' | t }}{% endcapture %}{% include 'ly-static-string' with '3335148' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
                <span class="input-group__btn">
                  <button type="submit" name="commit" class="btn btn--narrow">
                    {% include 'icon-arrow-right' %}
                    <span class="icon__fallback-text">{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_submit' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_submit' | t }}{% endcapture %}{% include 'ly-static-string' with '3335149' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
                  </button>
                </span>
              </div>
            {% endform %}
          </div>
          <p>{% comment %}ly_i18n_replace_for_[ 'general.password_page.admin_link_html' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.admin_link_html' | t }}{% endcapture %}{% include 'ly-static-string' with '3335150' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
        </div>
      </div>
      <button type="button" class="modal__close js-modal-close text-link">
        {% include 'icon-close' %}
        <span class="icon__fallback-text">{% comment %}ly_i18n_replace_for_[ 'general.accessibility.close_modal' | t | json ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.accessibility.close_modal' | t | json }}{% endcapture %}{% include 'ly-static-string' with '3335151' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | json }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
      </button>
    </div>

    {% comment %}ly_asset_replace_for_[ 'theme.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'theme.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}
  </body>
</html>
